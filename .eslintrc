{
    "root": true,
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-unused-vars": 0,
        "no-extra-boolean-cast": 0,
        "no-extra-semi": 0,
        "no-console": 0,
        "no-undef": 1,
    }
}
