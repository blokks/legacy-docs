{
  "name": "blokks-doc",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickpietens/blokks-docs"
  },
  "scripts": {
    "modernizr": "node_modules/modernizr/bin/modernizr -c modernizr.json -d src/static-source/scripts -u",
    "development": "npm run modernizr && npm run build:development",
    "staging": "npm update && npm run modernizr && npm run build:staging && npm run publish:staging && npm run open:staging",
    "production": "npm update && npm run modernizr && npm run build:production && npm run publish:production && npm run open:production",
    "build:development": "NODE_ENV=development gulp",
    "build:staging": "NODE_ENV=staging gulp",
    "build:production": "NODE_ENV=production gulp",
    "publish:staging": "rsync -avzq --del --exclude '.DS_Store' --exclude '.htaccess' dist/ blokkshq@blokks.co:~/webapps/staging_public/docs",
    "publish:production": "rsync -avzq --del --exclude '.DS_Store' --exclude '.htaccess' dist/ blokkshq@blokks.co:~/webapps/blokks_public/docs",
    "open:staging": "open https://staging.blokks.co/docs",
    "open:production": "open https://blokks.co/docs"
  },
  "browser": {},
  "browserify": {
    "transform": [
      "yamlify",
      "babelify",
      "require-globify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "modernizr": "global:Modernizr"
  },
  "devDependencies": {
    "babel-plugin-dead-code-elimination": "1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "6.16.0",
    "babelify": "7.3.0",
    "browser-sync": "^2.17.5",
    "browserify": "13.1.0",
    "browserify-shim": "^3.8.12",
    "core-decorators": "^0.13.0",
    "css-mqpacker": "^5.0.1",
    "del": "2.2.2",
    "envify": "3.4.1",
    "exorcist": "^0.4.0",
    "flow-bin": "^0.33.0",
    "gulp": "3.9.1",
    "gulp-changed": "1.3.2",
    "gulp-cssnano": "^2.1.2",
